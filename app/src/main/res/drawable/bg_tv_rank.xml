    <item>
        <shape android:shape="oval">
            <gradient android:startColor="#FF9329" android:endColor="#FDF25A" android:angle="-45"/>
        </shape>
    </item>

    <item android:top="3dp" android:bottom="3dp" android:right="3dp" android:left="3dp">
